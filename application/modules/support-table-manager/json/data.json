            "layouts_boxes_content": "<?php \n\n        \/\/Get all grids related to support tables\n        $grids = $this->db\n            ->where('entity_type', ENTITY_TYPE_SUPPORT_TABLE)\n            ->join('entity', 'entity_id = grids_entity_id', 'LEFT')\n            ->order_by('entity_name')\n        \t->limit(100)\n            ->get('grids')\n            ->result_array();\n\n \n\n?>                 \n\n<ul>\n\n<?php foreach ($grids as $griddb): ?>\n<?php $entity_name = explode('_', $griddb['entity_name']);\n$entity_name = implode(' ', $entity_name);\n$entity_name = ucfirst($entity_name);\n?>\n<li><a href=\"#\" class=\"toggle-link\" data-grid_id=\"<?php echo $griddb['grids_id'];?>\"><?php echo $entity_name;?><\/a> <small><?php echo str_replace(',', ' ',$griddb['entity_module']);?><\/small><\/li>\n<li style=\"list-style-type:none\">\n<div class=\"js_support_table_html toggle-div\" data-grid_html=\"<?php echo $griddb['grids_id'];?>\" style=\"display:none\">\n\n<\/div>\n<\/li>\n<?php endforeach;?>\n<\/ul>\n\n<script>\n  $(document).ready(function() {\n    \/\/ Aggiungiamo un event listener a tutti i link con classe \"toggle-link\"\n    $('.toggle-link').click(function(event) {\n      event.preventDefault(); \/\/ Per evitare di seguire il link\n\n      \/\/ Troviamo il data-target associato a questo link\n      \n   \tconst grid_id = $(this).data('grid_id');\n    const divToShowHide = $('div[data-grid_html=\"'+grid_id+'\"]');\n\n      \/\/ Esegui la chiamata AJAX all'URL \/support-table-manager\n      $.ajax({\n        url: '<?php echo base_url();?>support-table-manager\/support_table\/getSupportGrid\/'+grid_id,\n        method: 'GET',\n        success: function(data) {\n          \/\/ Inseriamo l'output nel div target\n          divToShowHide.html(data);\n\t\t\tinitComponents();\n          \/\/ Mostrare o nascondere il div target a seconda della situazione\n          divToShowHide.toggle();\n        },\n        error: function() {\n          console.log('Errore durante la chiamata AJAX.');\n        }\n      });\n    });\n  });\n<\/script>",
