            "fi_events_when": "pre-layout",
            "fi_events_json_data": "{\"_when\":\"pre\",\"fi_events_type\":\"hook\",\"hook\":{\"hooks_id\":\"321\",\"hooks_type\":\"layout\",\"hooks_ref\":\"\",\"hooks_module\":\"infobox\",\"hooks_order\":\"32\"},\"fi_events_action\":\"custom_code\",\"fi_events_actiondata\":{\"code\":\"<?php \\n$infobox_messages = $this->apilib->search(\\\"infobox\\\", [\\\"infobox_active\\\" => DB_BOOL_TRUE]);\\n\\nif (!empty($infobox_messages)):\\n?>\\n<script>\\nvar infobox_messages = '<?php echo base64_encode(json_encode($infobox_messages));?>';\\n\\n    function createInfobox(infobox) {\\n        \\\/\\\/ Controlla se l'infobox esiste gi\\u00e0\\n        if (document.getElementById('infobox_' + infobox.infobox_id)) {\\n            return; \\\/\\\/ Se esiste, interrompe la funzione\\n        }\\n\\n        var section = document.createElement('section');\\n        section.className = 'content-header';\\n        section.id = 'infobox_' + infobox.infobox_id; \\\/\\\/ Assegna un ID univoco\\n\\n        var div = document.createElement('div');\\n        div.className = 'callout ' + infobox.infobox_css_class;\\n        div.style.backgroundColor = infobox.infobox_color;\\n        div.style.color = \\\"#ffffff\\\";\\n        \\n        var h4 = document.createElement('h4');\\n        h4.textContent = infobox.infobox_title;\\n\\n        var p = document.createElement('p');\\n        p.textContent = infobox.infobox_message;\\n\\n        div.appendChild(h4);\\n        div.appendChild(p);\\n        section.appendChild(div);\\n\\n        var container = document.getElementById('js_layout_content_wrapper');\\n        if (container) {\\n            container.insertBefore(section, container.firstChild);\\n        }\\n    }\\n\\n    function displayInfoboxes() {\\n        \\n        var decodedString = atob(infobox_messages);\\n        var infoboxes = JSON.parse(decodedString);\\n\\n        infoboxes.forEach(createInfobox);\\n    }\\n\\n    displayInfoboxes();\\n<\\\/script>\\n\\n<?php\\nendif;\\n?>\\n\"},\"fi_events_title\":\"InfoBox Add Message \",\"save\":\"1\"}",
